format-version : 0.2
iteration-version : 1
name : "Test"
features :
    - name: "Base Test Feature"
      emacro: "E_TEST_BASE"
      richdescr: null
      deps:
      features:
      prop_groups:
        - group_name : "Test Group"
          props:
            - type : "string"
              name : "Prop String Test"
              emacro: "PROP_TEST_STRING"
              data : "Test Data"
              deps:
            - type : "chkbox"
              name : "Dependence Chkbox"
              emacro: "DEP_CHKBOX_1"
              data : True
              deps:
                -  dep_param : "availability"
                   dep_condition : "on_target_availability"
                   target_kind : "property_by_emacro"
                   target: "DEP_CHKBOX_2"
            - type : "chkbox"
              name : "Dependence Chkbox 2"
              emacro: "DEP_CHKBOX_2", data : true
              data : False
              deps:
                -  dep_param : "availability"
                   dep_condition : "on_target_availability"
                   target_kind : "property_by_name"
                   target: "Dependence Chkbox 3"
            - type : "chkbox"
              name : "Dependence Chkbox 3"
              emacro: "DEP_CHKBOX_3"
              data : True
              deps:


    - name: "Empty Dependent Test Feature"
      emacro: "E_TEST_EMPTY_EXAMPLE"
      richdescr: null
      deps:
        - dep_param : "enabled"
          dep_condition : "on_target_enabled"
          target_kind : "feature_by_emacro"
          target : "E_TEST_BASE"
      features:
      prop_groups: